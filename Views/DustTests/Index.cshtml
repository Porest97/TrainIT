@model IEnumerable<TrainIT.Models.DataModels.DustTest>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateTimePreformed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Arena)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Sport)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TestedPerson)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TimeSet1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TimeSet2)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TimeSet3)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TimeSet4)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TimeTotal)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.DateTimePreformed)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Arena.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Sport.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TestedPerson.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeSet1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeSet2)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeSet3)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeSet4)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeTotal)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
